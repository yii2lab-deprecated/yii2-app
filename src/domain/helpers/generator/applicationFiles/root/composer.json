{
	"name": "group/yii2-project",
	"description": "Web application",
	"keywords": [
		"application",
		"web",
	],
	"type": "project",
	"minimum-stability": "stable",
	"require": {
		"yii2lab/yii2-app": "1.38.*",
		"yii2lab/yii2-application-template": "0.19.*",
		"yii2lab/yii2-init": "1.19.*",
		"yii2lab/yii2-console": "1.12.*",
		"yii2lab/yii2-rbac": "1.27.*",
		"yii2lab/yii2-store": "1.7.*",
		"yii2lab/yii2-rest": "1.36.*",
		"yii2lab/yii2-helpers": "1.51.*",
		"yii2lab/yii2-misc": "1.20.*",
		"yii2lab/yii2-ubuntu-font": "1.3.*",
		"yii2lab/yii2-validator": "1.8.*",
		"yii2lab/yii2-domain": "1.69.*",
		"yii2lab/yii2-extension": "0.36.*",
		"yii2lab/yii2-notify": "1.22.*",
		"yii2lab/yii2-widgets": "1.8.*",
		"yii2lab/yii2-core": "1.18.*",
		"yii2lab/yii2-navigation": "1.6.*",
		"yii2lab/yii2-db": "1.18.*",
		"yii2lab/yii2-design-pattern": "0.7.*",
		"yii2module/yii2-error": "1.7.*",
		"yii2module/yii2-lang": "2.19.*",
		"yii2module/yii2-markdown": "0.9.*",
		"yii2module/yii2-dashboard": "0.8.*",
		"yii2module/yii2-account": "1.66.*",
		"yii2module/yii2-summary": "0.4.*",
		"alex-lyanik/yii2-viewlog": "dev-master"
	},
	"require-dev": {
		"yii2lab/yii2-test": "1.266.*",
		"yii2module/yii2-guide": "1.23.*",
		"yii2module/yii2-vendor": "0.44.*",
		"yii2module/yii2-tool": "0.9.*",
		"yii2guide/markdown": "0.0.*",
		"yii2guide/misc": "0.0.*",
		"yii2guide/perfect-code": "0.0.*",
		"yii2guide/team": "0.0.*"
	},
	"config": {
		"secure-http": false,
		"process-timeout": 1800
	},
	"extra": {
		"installer-types": [
			"bower-asset",
			"npm-asset"
		],
		"installer-paths": {
			"vendor/{$vendor}/{$name}/": [
				"type:bower-asset",
				"type:npm-asset"
			]
		}
	},
	"repositories": [
		{
			"type": "composer",
			"url": "https://asset-packagist.org"
		}
	]
}